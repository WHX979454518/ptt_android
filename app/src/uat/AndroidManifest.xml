<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
                 xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application android:name="com.xianzhitech.ptt.UatApp"
                 tools:replace="android:name" >

        <activity android:name="net.hockeyapp.android.UpdateActivity" />

        <activity android:name="com.xianzhitech.ptt.FeedbackActivity"
                  android:label="@string/app_name_feedback">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="net.hockeyapp.android.FeedbackActivity" android:theme="@android:style/Theme.Holo" />
        <activity android:name="net.hockeyapp.android.PaintActivity" />

    </application>

</manifest>
